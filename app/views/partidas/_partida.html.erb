<table class="table table-striped">
    <tbody>        
        <tr>
            <td><strong><%= t('.calidad_cafe') %></strong></td>
            <td><%= partida.calidad_cafe %></td>
        </tr>
        <tr>
            <td><strong><%=  t('.tipo_cafe') %></strong></td>
            <td><%= partida.type_coffee.name %></td>
        </tr>                              
        <tr>
            <td><strong><%= t('.humedad') %></strong></td>
            <td><%= "#{number_with_precision(partida.humedad, precision: 2)} %" %></td>
        </tr>
        <tr>
            <td><strong><%= t('.sacos') %></strong></td>
            <td><%= render "partidas/sacos", partida: partida %></td>
        </tr>
        <tr>
            <td><strong><%= t('.bolsas') %></strong></td>
            <td><%= render "partidas/bolsas", partida: partida %></td>
        </tr>
        <tr>
            <td><strong><%=  t('.kilogramos_brutos') %></strong></td>
            <td><%= "#{number_with_precision(partida.kilogramos_brutos, precision: 2)} Kgs" %></td>
        </tr>                              
        <tr>
            <td><strong><%=  t('.tara') %></strong></td>
            <td><%= "#{number_with_precision(partida.tara, precision: 2)} Kgs" %></td>
        </tr>                              
        <tr>
            <td><strong><%=  t('.kilogramos_netos') %></strong></td>
            <td>
              <div class="row">
                <%= render "partidas/kilogramos_netos", partida: partida %>
                <%= render "salidas/salida_proceso", partida: partida, line_item_salida_proceso: @line_item_salida_proceso %>                  
                <%= render "salidas/salida_bodega", partida: partida, line_item_salida_bodega: @line_item_salida_bodega %>                  
              </div>
            </td>
        </tr>                              
        <tr>
            <td><strong><%=  t('.observaciones') %></strong></td>
            <td><%= partida.observaciones %></td>
        </tr>                              
    </tbody>
</table>