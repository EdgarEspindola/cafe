<p id="notice"><%= notice %></p>

<h1>Cart Salidas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cart_salidas.each do |cart_salida| %>
      <tr>
        <td><%= link_to 'Show', cart_salida %></td>
        <td><%= link_to 'Edit', edit_cart_salida_path(cart_salida) %></td>
        <td><%= link_to 'Destroy', cart_salida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cart Salida', new_cart_salida_path %>
